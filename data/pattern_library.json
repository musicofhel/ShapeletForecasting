    "name": "Head and Shoulders",
    "category": "traditional",
    "description": "Reversal pattern with three peaks, middle highest",
    "key_points": [
      {
        "position": 0.0,
        "type": "start"
      },
      {
        "position": 0.2,
        "type": "left_shoulder_peak"
      },
      {
        "position": 0.3,
        "type": "left_valley"
      },
      {
        "position": 0.5,
        "type": "head_peak"
      },
      {
        "position": 0.7,
        "type": "right_valley"
      },
      {
        "position": 0.8,
        "type": "right_shoulder_peak"
      },
      {
        "position": 1.0,
        "type": "end"
      }
